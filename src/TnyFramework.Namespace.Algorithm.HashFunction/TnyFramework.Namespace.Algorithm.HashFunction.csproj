<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net47;net471;net472;net48;netstandard2.0</TargetFrameworks>
        <Configurations>Debug;Release;Publish</Configurations>
        <Platforms>AnyCPU</Platforms>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <LangVersion>7.3</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\TnyFramework.Namespace.Algorithm\TnyFramework.Namespace.Algorithm.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="System.Data.HashFunction.CityHash" Version="2.0.0" />
        <PackageReference Include="System.Data.HashFunction.FarmHash" Version="2.0.0" />
        <PackageReference Include="System.Data.HashFunction.MetroHash" Version="2.0.0" />
        <PackageReference Include="System.Data.HashFunction.MurmurHash" Version="2.0.0" />
        <PackageReference Include="System.Data.HashFunction.xxHash" Version="2.0.0" />
    </ItemGroup>

</Project>
